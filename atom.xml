layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>hyunjong-lee</title>
 <link href="http://hyunjong-lee.github,io/atom.xml" rel="self"/>
 <link href="http://hyunjong-lee.github,io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://hyunjong-lee.github,io/</id>
 <author>
   <name>hyunjong-lee</name>
   <email>hyunjong.lee.s@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ site.url }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ site.url }}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>